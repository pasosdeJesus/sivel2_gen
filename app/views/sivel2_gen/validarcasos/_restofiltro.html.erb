
<div class="row filtro-fila">
  <div class="offset-sm-0 col-sm-2 offset-md-1 col-md-1">
    Etiqueta
  </div>
  <div class="offset-sm-0 col-sm-4">
    <%= f.input :etiqueta_id,
      collection: Msip::ModeloHelper.opciones_tabla_basica(
        Msip::Etiqueta, f, :etiqueta_id
      ),
      selected: (
        params[:validarcaso] ? 
        params[:validarcaso][:etiqueta_id] :
        ''
      ),
      include_blank: true,
