$("#numconscaso").html("<%= @numconscaso %>");

<%if @numconscaso <= Rails.configuration.x.sivel2_consweb_max.to_i || 
    current_usuario %>
  $("#casos").html("<%= escape_javascript(
    render('sivel2_gen/casos/index_tabla')) %>")
<% else %>
    $("#casos").html('<div id="mensmaxpasado" class="alert alert-danger">'+
      '<h4><strong>La consulta pública permite máximo '+
      '<%= Rails.configuration.x.sivel2_consweb_max %> registros.</strong><h4>'+
      '<p>Restrinja más el filtro (por ejemplo a un año) para obtener '+
      ' menos registros.'+
      ' <%= Rails.configuration.x.sivel2_consweb_epilogo ? 
          Rails.configuration.x.sivel2_consweb_epilogo : '' %>'+
      ' </p></div>')
<% end %>
