<div  style="background: #730E15; color: white">
  <div class="col-md-6">
    <%= f.input :fecha_localizada, 
      :input_html => { "data-behaviour" => "datepicker" },
      :as => :string %>
  </div>
  <div class="col-md-6">
    <%= f.input :id, readonly: true%>
  </div>
</div>
<div style="background: #730E15; color: white">
    <%= f.input :titulo %>
</div>
<div  style="background: #730E15; color: white">
  <div class="col-md-4">
    <%= f.input :hora %> <!--, as: :time, ampm: true -->
  </div>
  <div class="col-md-4">
    <%= f.input :duracion %>
  </div>
  <div class="col-md-4">
    <%= f.association :intervalo,
      collection: Sivel2Gen::Intervalo.habilitados,
      label_method: :nombre,
      value_method: :id %>
  </div>
</div>

<div style="background: #730E15; color: white">
    <%= render partial: 'basicos_final', locals: {f: f} %>
</div>
