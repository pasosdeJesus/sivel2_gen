
<div class="row">
  <div class="col-md-6">
    <%= f.association :contexto, 
      collection: Sip::ModeloHelper.opciones_tabla_basica(
        Sivel2Gen::Contexto, f, :contexto
      ),
      label_method: :nombre, 
      value_method: :id %>
  </div>
  <div class="col-md-6">
    <% cant = Sip::ModeloHelper.opciones_tabla_basica(
      Sivel2Gen::Antecedente, f, :antecedente) %>
    <%= f.association :antecedente, 
      collection: cant,
      label_method: :nombre, 
      value_method: :id %>
  </div>
</div>

<%= f.input :bienes%>
